<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/networkName"
        android:layout_width="match_parent"
        android:textSize="@dimen/textSize_16"
        android:fontFamily="@font/poppins_bold"
        android:background="@drawable/gray_button_fon"
        android:textColor="@color/dark_text_color"
        android:elegantTextHeight="true"
        android:layout_marginEnd="@dimen/displayMarginSize"
        android:layout_marginStart="@dimen/displayMarginSize"
        android:layout_marginBottom="@dimen/dp_10"
        android:paddingBottom="@dimen/dp_16"
        android:paddingTop="@dimen/dp_16"
        android:paddingStart="@dimen/dp_22"
        android:paddingEnd="@dimen/dp_22"
        android:layout_height="wrap_content">

    </androidx.appcompat.widget.AppCompatTextView>

</androidx.appcompat.widget.LinearLayoutCompat>
